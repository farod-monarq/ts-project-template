{
  "name": "@mock-service/grpc-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf --glob dist *.tsbuildinfo .eslintcache",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "npx eslint \"**/*.ts\"",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "mocha dist/__tests__",
    "posttest": "npm run lint",
    "test:dev": "mocha dist/__tests__/**/*.js && npm run posttest",
    "prepare": "npm run build",
    "rebuild": "npm run clean && npm run build",
    "start": "node -r source-map-support/register ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-mocha": "^10.1.0",
    "google-protobuf": "^3.21.2",
    "grpc-tools": "^1.12.4",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "protoc-gen-ts": "^0.8.7",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@loopback/filter": "^4.1.5",
    "@mock-service/core": "workspace:^",
    "axios": "^1.6.5",
    "express": "^4.18.2",
    "tslib": "^2.5.0"
  }
}
